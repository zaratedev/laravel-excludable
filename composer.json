{
    "name": "zaratedev/laravel-excludable",
    "description": "Exclude the provided elements from the query results.",
    "keywords": [
        "exclude",
        "eloquent-models",
        "laravel-excludable"
    ],
    "homepage": "https://github.com/zaratedev/laravel-excludable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jonathan Zarate",
            "email": "zaratedev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ZarateDev\\LaravelExcludable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZarateDev\\LaravelExcludable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
